install.packages("lmtest")
library('lmtest')
bp=c(105,115,116,117,112,121,121,110,110,114,114,115,114,106,125,114,106,113,110,122)
age=c(47,49,49,50,51,48,49,47,49,48,47,49,50,45,52,46,46,46,48,56)
w=c(85.4,94.2,95.3,94.7,89.4,99.5,99.8,90.9,89.2,92.7,94.4,94.1,91.6,87.1,101.3,94.5,87,94.5,90.5,95.7)
bsa=c(1.75,2.1,1.98,2.01,1.89,2.25,2.25,1.9,1.83,2.07,2.07,1.98,2.05,1.92,2.19,1.98,1.87,1.9,1.88,2.09)
dur=c(5.1,3.8,8.2,5.8,7,9.3,2.5,6.2,7.1,5.6,5.3,5.6,10.2,5.6,10,7.4,3.6,4.3,9,7)
pul=c(63,70,72,73,72,71,69,66,69,64,74,71,68,67,76,69,62,70,71,75)
str=c(33,14,10,99,95,10,42,8,62,35,90,21,47,80,98,95,18,12,99,99)
model1=lm(bp~age+w+bsa+dur+pul+str);model1
gqtest(model1,order.by=~age+w+bsa+dur+pul+str,fraction=0.2)
