Y=c(4.58,2.86,3.02,2.29,1.61,6.87,3.85,6.12,3.29,5.88,4.2,3.85,1.99,4.22,2.76,2.55)
x1=c(4.99,16.11,9.75,10.65,20.01,5.97,8.57,5.24,15.79,8.26,7.03,2.96,8.86,5.49,8.63,20.31)
x2=c(69,73,49,61,28,30,46,25,43,23,23,21,22,9,12,12)
x3=c(8,8,10,13,12,6,8,9,12,7,6,8,9,11,8,7)
x4=c(0.200400802,0.062073246,0.102564103,0.093896714,0.049975012,2.007504188,0.816686114,0.570839695,1.453331222,1.331065375,1.992247511,0.337837838,0.112866817,0.362149362,0.115874855,1.039236829)
x5=c(8,4,4,6,4,4,4,4,4,4,4,4,4,2,2,4)
x6=c(4.6,4.4,4.7,3.8,2.2,24.8,11,18.5,7.5,8.2,5.4,5.4,3.2,8.9,12.4,7.8)
x7=c(1.2,1.43,1.54,0.94,0.65,0.34,0.47,0.38,0.95,0.12,0.29,0.34,0.68,0.18,0.14,0.05)
length(x7)
model3=lm(Y~x1+x2+x3+x4+x5+x6+x7);model3
summary(model3)
data=data.frame(Y,x1,x2,x3,x4,x5,x6,x7)
#perform foward selection
null_model=lm(Y~1,data=data)

forward_model=step(null_model,scope= list(lower=formula(null_model),upper=formula(model3)),direction="forward")
summary(forward_model)
stepwise=step(model3,direction="both")
summary(stepwise)
backward_model= step(model3,direction = "backward")
summary(backward_model)
